{
    "name": "@lwys-pkg-releaser/node-bullshit-generator",
    "version": "1.2.0",
    "description": "Port of menzi11/BullshitGenerator@github.com to JavaScript",
    "main": "./src/狗屁库.js",
    "browser": "./dist/狗屁库.js",
    "unpkg": "./dist/狗屁库.js",
    "bin": {
        "bullshit-generator": "./bin/bs-gen.js"
    },
    "directories": {
        "src": "src",
        "lib": "lib",
        "bin": "bin"
    },
    "browserslist": [
        "cover 99.5%",
        "last 5 years",
        "Firefox ESR",
        "not dead",
        "maintained node versions"
    ],
    "scripts": {
        "test": "node ./bin/bs-gen.js 学生会退会",
        "build": "npm run bundle+min",
        "clean": "rm -r ./dist ./_babel_lib",
        "babel": "babel src --out-dir ./_babel_lib",
        "bundle": "npm run babel && browserify -e ./_babel_lib/狗屁库.js -s bullshit -o ./dist/狗屁库.js",
        "bundle+min": "npm run bundle && npm run minify-bundle",
        "minify-bundle": "terser ./dist/狗屁库.js -o ./dist/狗屁库.min.js -m -c --timings --verbose --warn",
        "prepack": "npm run bundle+min",
        "postpack": "npm run clean"
    },
    "author": {
        "name": "Liu Wenyuan",
        "email": "liuwenyuanbackup@gmail.com",
        "url": "https://dobby233liu.github.io/?src=npm"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dobby233Liu/node-bullshit-generator.git"
    },
    "keywords": [
        "text",
        "generator",
        "bullshit",
        "chinese",
        "nonsense",
        "meaningless"
    ],
    "license": "SEE LICENSE IN LICENSE.txt",
    "bugs": {
        "url": "https://github.com/Dobby233Liu/node-bullshit-generator/issues"
    },
    "homepage": "https://github.com/Dobby233Liu/node-bullshit-generator#readme",
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-transform-classes": "^7.14.5",
        "@babel/preset-env": "^7.9.0",
        "browserify": "^17.0.0",
        "core-js": "^3.6.4",
        "prettier": "2.5.1",
        "terser": "^5.7.0"
    },
    "files": [
        "*",
        "dist"
    ]
}
